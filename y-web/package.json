{
  "name": "y-web",
  "description": "Web client for y",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit --skipLibCheck && vite build",
    "build:force": "vite build",
    "build:sourcemap": "vite build --sourcemap && source-map-explorer dist/assets/*.js",
    "serve": "vite preview",
    "lint": "eslint .",
    "prepare": "cd .. && husky install y-web/.husky"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.42.0",
    "eslint-plugin-solid": "^0.12.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "husky": "^8.0.3",
    "less": "^4.2.0",
    "prettier": "^3.3.3",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.1.6",
    "vite": "^5.4.11",
    "vite-plugin-solid": "^2.10.2"
  },
  "dependencies": {
    "@solid-primitives/i18n": "^2.1.1",
    "@solid-primitives/websocket": "^1.2.2",
    "@solidjs/router": "^0.8.3",
    "@tanstack/solid-query": "^4.32.6",
    "date-fns": "^4.1.0",
    "material-symbols": "^0.27.0",
    "solid-js": "^1.9.3",
    "zod": "^3.23.8"
  }
}
